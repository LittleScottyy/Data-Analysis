    "# Bin the age data using the split points\n",
    "stroke_data['age_binned'] = np.digitize(stroke_data['age'], bins=split_points, right=False)\n",
    "\n",
    "# Output the DataFrame with binned age\n",
    "print(stroke_data[['age', 'age_binned']])"
